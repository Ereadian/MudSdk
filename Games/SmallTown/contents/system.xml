<?xml version="1.0"?>
<resources name="SystemResources">
  <resource name="SelectLocale">
    <content locale="zh-cn">请输入语言编号:</content>
    <content locale="en-us">Please enter id of langauge:</content>
  </resource>
  <resource name="InvalidLocaleId">
    <content locale="zh-cn">{%red%}错误的语言编号，请重新输入。{%%}</content>
    <content locale="en-us">{%red%}Invalid language id. Please try again.{%%}</content>
  </resource>
  <resource name="EnterUserName">
    <content locale="zh-cn">请输入用户名</content>
    <content locale="en-us">Please enter user name</content>
  </resource>
  <resource name="EnterPassword">
    <content locale="zh-cn">请输入密码</content>
    <content locale="en-us">please enter passwod</content>
  </resource>
  <resource name="InvalidName">
    <content locale="zh-cn">{%red%}用户名只能包含字母数字和小数点{%%}</content>
    <content locale="en-us">{%red%}User name should contain letter, number and dot only{%%}</content>
  </resource>
  <resource name="InvalidPassword">
    <content locale="zh-cn">{%red%}密码不正确，请重试。{%%}</content>
    <content locale="en-us">{%red%}Password is not correct. Please try again.{%%}</content>
  </resource>
  <resource name="NewUser">
    <content locale="zh-cn">欢迎加入游戏。系统正在为您建立账号。请输入您新账号的密码:</content>
    <content locale="en-us">Welcome to the game! we are creating profile for you. Please enter password for your account:</content>
  </resource>
  <resource name="ConfirmPassword">
    <content locale="zh-cn">请再次输入您的密码:</content>
    <content locale="en-us">Please re-enter password:</content>
  </resource>
  <resource name="PasswordNotMatch">
    <content locale="zh-cn">{%red%}密码不一致。请重新创建密码。{%%}</content>
    <content locale="en-us">{%red%}Password does not match. Please re-create password.{%%}</content>
  </resource>
  <resource name="CreatingAccount">
    <content locale="zh-cn">创建游戏账号中。。。</content>
    <content locale="en-us">Creating account ...</content>
  </resource>
  <resource name="LoadingUserWorldData">
    <content locale="zh-cn">加载玩家数据。。。</content>
    <content locale="en-us">Loading player game data ...</content>
  </resource>
</resources>